{
  "mcpServers": {
      "sequential-thinking": {
          "command": "npx",
          "args": [
              "-y",
              "@zengwenliang/mcp-server-sequential-thinking"
          ]
      },
      "software-planning-tool": {
        "command": "npx",
        "args": [
            "-y",
              "@zengwenliang/software-planning-tool"
          ],
          "autoApprove": []
      },
      "filesystem": {
          "command": "npx",
          "args": [
              "-y",
              "@modelcontextprotocol/server-filesystem",
              "/",
              "/"
          ]
      },
      "package-version": {
          "command": "npx",
          "args": [
              "-y",
              "mcp-package-version"
          ]
      },
      "puppeteer": {
          "command": "npx",
          "args": [
              "-y",
              "@modelcontextprotocol/server-puppeteer"
          ]
      },
      "postgres": {
        "command": "npx",
        "args": [
          "-y",
          "@modelcontextprotocol/server-postgres",
          "postgresql://localhost:5432/monitor"
        ]
      }
  }
}